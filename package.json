{
  "name": "ugmk_test_app",
  "version": "1.0.0",
  "private": true,
  "description": "React app with simple API",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "start:server": "node server",
    "dev": "nodemon server",
    "build": "react-scripts build",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "devServer": "json-server -w data/products.json -p 3001",
    "dockerize": "docker build . -t ugmk_test_app",
    "start-container": "docker run --rm -p 3000:3000 --name ugmk_test_app -d ugmk_test_app"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Vitaly Agarkov",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.3.6",
    "chart.js": "^4.2.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "csv-parse": "^5.3.8",
    "date-fns": "^2.29.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "redux-saga": "^1.2.3",
    "saga-toolkit": "^2.0.6"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "json-server": "^0.17.3",
    "nodemon": "^2.0.22"
  }
}
